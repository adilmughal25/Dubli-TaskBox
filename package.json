    "cheerio": "^0.19.0",
    "co": "^4.6.0",
    "co-fs": "^1.2.0",
    "co-request": "^1.0.0",
    "co-waiter": "^1.0.2",
    "cofy": "^2.0.2",
    "csvtojson": "^1.0.3",
    "deasync": "^0.1.9",
    "debug": "^2.2.0",
    "deep": "0.0.2",
    "deep-freeze": "0.0.1",
    "denodeify": "^1.2.1",
    "fs-promise": "^0.3.1",
    "ftp": "^0.3.10",
    "ftpd": "^0.2.11",
    "handlebars": "^3.0.3",
    "hat": "0.0.3",
    "hiredis": "^0.4.1",
    "html-entities": "^1.1.2",
    "iconv-lite": "^0.4.11",

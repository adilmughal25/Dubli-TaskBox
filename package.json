{
  "name": "ominto-taskbox",
  "version": "0.0.1",
  "description": "DubLi TaskBox Server",
  "main": "app.js",
  "scripts": {
    "start": "npm install && node --harmony src/server.js | bunyan",
    "test": "npm install && node --harmony node_modules/.bin/_mocha --timeout 10000 test/unit/*.js",
    "coverage": "npm install && node --harmony node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --timeout 10000 test/unit/*.js",
    "integration": "npm install && node --harmony node_modules/.bin/_mocha --timeout 50000 test/integration/*.js",
    "debug": "npm install && node --debug --harmony src/server.js | bunyan",
    "task-list": "node --harmony ./src/bin/task-runner.js --list",
    "task": "node --harmony ./src/bin/task-runner.js --run"
  },
  "engines": {
    "node": "0.12.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dubli/taskbox.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dubli/issues/issues"
  },
  "homepage": "http://dubli.com",
  "dependencies": {
    "async": "^2.0.1",
    "aws-sdk": "^2.2.30",
    "aws-sdk-promise": "0.0.2",
